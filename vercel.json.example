{
  "crons": [
    {
      "path": "/api/send-reminder?secret=your_cron_secret_here",
      "schedule": "0 20 * * *"
    },
    {
      "path": "/api/send-scheduled-reminder?secret=your_cron_secret_here",
      "schedule": "*/30 * * * *"
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
